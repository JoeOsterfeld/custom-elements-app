{
  "name": "minfw-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:client\" \"npm run dev:server\"",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "vite -c vite.config.client.ts",
    "build:client": "tsc --project ./tsconfig.client.json && vite build -c vite.config.client.ts",
    "build:server": "tsc --project ./tsconfig.server.json",
    "dev:server": "nodemon --watch ./src/server --exec \"tsx ./src/server/index.ts\" ./src/server/index.ts",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsdom": "^24.0.0",
    "minfw": "^1.2.1"
  }
}
